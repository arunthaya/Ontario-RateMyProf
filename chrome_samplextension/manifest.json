{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": [
     "https://schoolsearchengine.herokuapp.com/",
     "activeTab",
     "declarativeContent",
     "storage"
    ],
    "background": {
     "scripts": ["background.js"],
     "persistent": false
    },
    "page_action": {
     "default_popup":"popup.html",
     "default_icon": {
       "16": "images/get_started16.png",
       "32": "images/get_started32.png",
       "48": "images/get_started48.png",
       "128": "images/get_started128.png"
     }
    },
    "content_scripts": [
      {
        "matches":["https://central.carleton.ca/prod/*course_search"],
        "run_at":"document_idle",
        "js":["contentScript.js"]
      }
    ],
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "author":"arun thayanithy",
    "manifest_version": 2
 }
