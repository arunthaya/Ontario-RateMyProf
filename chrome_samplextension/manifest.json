{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": [
      "https://schoolsearchengine.herokuapp.com/",
      "activeTab",
      "declarativeContent",
      "storage"
    ],
    "content_scripts":[
      {
        "matches":["https://central.carleton.ca/prod/*course_search"],
        "css":[
          "/jquery.ui/jquery-ui.css",
          "/jquery.ui/jquery-ui.min.css",
          "/jquery.ui/jquery-ui.structure.css",
          "/jquery.ui/jquery-ui.structure.min.css",
          "/jquery.ui/jquery-ui.theme.css",
          "/jquery.ui/jquery-ui.theme.min.css",
          "jquery.qtip.min.css"
        ],
        "js":["jquery-3.3.1.min.js","jquery.qtip.min.js","/jquery.ui/jquery-ui.js","contentScript.js"]
      }
    ],
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "author":"arun thayanithy",
    "manifest_version": 2
 }
