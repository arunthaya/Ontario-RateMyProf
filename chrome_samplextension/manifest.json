{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": [
      "https://ratemyprofchrome.herokuapp.com/",
      "activeTab",
      "declarativeContent",
      "storage",
      "*://*/*"
    ],
    "browser_action":{
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      },
      "default_title": "Unofficial Rate My Prof",
      "default_popup": "popup.html"
    },
    "author":"arun thayanithy",
    "manifest_version": 2
 }
