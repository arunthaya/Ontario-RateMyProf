{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": [
      "https://ratemyprofchrome.herokuapp.com/",
      "background",
      "activeTab",
      "declarativeContent",
      "storage",
      "*://*/*"
    ],
    "background": {
      "persistent": false,
      "scripts": [
        "jquery-3.3.1.min.js",
        "jquery.qtip.min.js",
        "background.js"
      ],
      "css":["jquery.qtip.min.css"]
    },
    "browser_action":{
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      },
      "default_title": "Unofficial Rate My Prof",
      "default_popup": "popup.html"
    },
    "options_page":"options.html",
    "web_accessible_resources":[
      "style/jquery.qtip.min.css"
    ],
    "author":"arun thayanithy",
    "manifest_version": 2
 }
