{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": [
      "https://ratemyprofchrome.herokuapp.com/",
      "background",
      "activeTab",
      "declarativeContent",
      "storage",
      "*://*/*"
    ],
    "background": {
      "persistent": false,
      "scripts": [
        "jquery-3.3.1.min.js",
        "jquery.qtip.min.js",
        "background.js"
      ],
      "css":["jquery.qtip.min.css"]
    },
    "browser_action":{
      "default_icon": {
        "16": "images/teacher_rmp16.png",
        "32": "images/teacher_rmp32.png",
        "48": "images/teacher_rmp48.png",
        "128": "images/teacher_rmp128.png"
      },
      "default_title": "Unofficial Rate My Prof",
      "default_popup": "popup.html"
    },
    "options_ui":{
      "page":"options.html",
      "open_in_tab":false
    },
    "web_accessible_resources":[
      "style/jquery.qtip.min.css"
    ],
    "author":"arun thayanithy",
    "manifest_version": 2
 }
